@section HeadIncludes
{
    <meta charSet="utf-8" class="next-head" />
    @foreach (var refs in Model.CSSRefs)
    {
        <link rel="stylesheet" type="text/css" href="@refs" />
    }
}

<!-- ConfiguraciÃ³n -->
<script id="__NEXT_DATA__" type="application/json">
    {
    "dataManager": "[]",
    "props": {
    "pageProps": {
    "user":@Html.Raw(Model.User),
    "config":@Html.Raw(Model.Config)
    }
    },
    "page": "/",
    "query": {},
    "buildId": "@Model.BuildId",
    "nextExport": true,
    "assetPrefix" : "@Model.CndRoot.Substring(0, Model.CndRoot.LastIndexOf("_next/") - 1)"
    }
</script>

<script id="__NEXT_PAGE__/" src="@Model.CndRoot/static/@Model.BuildId/pages/index.js" async></script>
<script id="__NEXT_PAGE__/_app" src="@Model.CndRoot/static/@Model.BuildId/pages/_app.js" async></script>

@foreach (var refs in Model.JSRefs)
{
    <script src="@refs" async=""></script>
}